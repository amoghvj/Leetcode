//C

void merge(int* nums1, int nums1Size, int m, int* nums2, int nums2Size, int n) {
    int swap = 0;
    for (int i = 0; i < nums1Size; i++) {
        int k = i;
        for (int j = i; j < nums1Size; j++) {
            if (j - swap >= m) {
                nums1[j] = nums2[j - m];
                swap++;
            }
            if (nums1[j] < nums1[k])
                k = j;
        }
        int temp = nums1[i];
        nums1[i] = nums1[k];
        nums1[k] = temp;
    }
}
