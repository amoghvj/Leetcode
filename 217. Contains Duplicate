/*Can take powers of 2: 2, 4, 8... in one int, then powers of 2 + 1 in next int until we reach 2 x (10^9)*/
#include <stdio.h>
#include <stdbool.h>
bool containsDuplicate(int* nums, int numsSize) {
    int cache[200000002];
    for(int i = 0; i < numsSize; i++) {
        int index = nums[i];
        if(index < 0) index += 200000002;
        if(cache[index]) return true;
        cache[index] = 1;
    }
    return false;
}
