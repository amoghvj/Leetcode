//C

#include <stdbool.h>
bool hasGroupsSizeX(int* deck, int deckSize) {
    int cache[10001] = {0}, partition = 0;
    for (int i = 0; i < deckSize; i++)
        cache[deck[i]]++;
    for (int i = 0; i < deckSize; i++) {
        if (!partition)
            partition = cache[deck[i]];
        else
            partition = GCD(partition, cache[deck[i]]);
        if (partition < 2)
            return false;
    }
    return true;
}

int GCD(int a, int b) {
    if (b > a) {
        a += b;
        b = a - b;
        a = a - b;
    }
    if (a % b == 0)
        return b;
    else
        return GCD(b, a % b);
}
