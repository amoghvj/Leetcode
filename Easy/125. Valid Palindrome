//C

#include <stdio.h>
#include <string.h>
#include <ctype.h>
bool isPalindrome(char* s) {
    char process[strlen(s)];
    int j = 0;
    for(int i = 0; i < strlen(s); i++) {
        if(isalpha(s[i]) || isdigit(s[i])) {
            process[j] = tolower(s[i]);
            j++;
        }
    }
    printf(process);
    for(int i = 0; i < (j + 1) / 2; i++) if(process[i] != process[j - i - 1]) return false;
    return true;
}
