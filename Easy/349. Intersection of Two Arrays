//C

/**
 * Note: The returned array must be malloced, assume caller calls free().
 */
int* intersection(int* nums1, int nums1Size, int* nums2, int nums2Size, int* returnSize) {
    int* out = (int*)malloc(1000 *sizeof(int)), cache[1001] = {0}, count = 0;
    for(int i = 0; i < nums1Size; i++) cache[nums1[i]] = 1;
    for(int i = 0; i < nums2Size; i++) {
        if(cache[nums2[i]]) {
            out[count++] = nums2[i];
            cache[nums2[i]] = 0;
        }
    }
    *returnSize = count;
    return out;
}
