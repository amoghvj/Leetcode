//C

#include <stdio.h>
#include <stdbool.h>
bool isIsomorphic(char* s, char* t) {
    char Ascii[128], Bscii[128];
    for (int i = 0; i < strlen(s); i++) {
        if (Ascii[s[i]]) {
            if (t[i] == Ascii[s[i]])
                continue;
            return false;
        }
        if (Bscii[t[i]])
            return false;
        Ascii[s[i]] = t[i];
        Bscii[t[i]] = s[i];
    }
    return true;
}
