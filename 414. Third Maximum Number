//C

int thirdMax(int* nums, int numsSize) {
    int i = 0, dup = 0;
    for (; i < 3 && i < numsSize; i++) {
        int k = i, temp = nums[i];
        for (int j = i + 1; j < numsSize - dup; j++) {
            if (nums[j] == nums[k]) {
                int temp2 = -2147483648;
                nums[j] = nums[numsSize - dup - 1];
                nums[numsSize - dup - 1] = temp2;
                dup++;
            }
            if (nums[j] > nums[k]) {
                k = j;
            }
        }
        nums[i] = nums[k];
        nums[k] = temp;
    }
    if (numsSize - dup < 3)
        i = 1;
    return nums[i - 1];
}
