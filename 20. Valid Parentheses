//Javascript

/**
 * @param {string} s
 * @return {boolean}
 */
const brack1 = ['(', '{', '['], brack2 = [')', '}', ']'];
var isValid = function (s) {
    var brackets = [];
    for (let i = 0; i < s.length; i++) {
        ch = s[i];
        if (brack2.includes(ch)) {
            if(brackets[brackets.length - 1] == brack1[brack2.findIndex(val => val == ch)]) brackets.pop();
            else return false;
        } else {
            if (['(', '{', '['].includes(ch)) brackets.push(ch);
            continue;
        }
    }
    return brackets.length == 0;
};
